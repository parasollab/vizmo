all:
	g++ -o exe -O3 -I../ -I../../common -D_gplusplus=1 -DQUIETGRAPH main.cc
all3:
	g++3 -o exe -g -I../ -I../../common -D_gplusplus=1 -D_EXPLICIT_USE_NAMESPACE_STD main.cc
old:
	g++ -o oldexe -g -D_gplusplus=1 -DQUIETGRAPH=1 oldmain.cc
sgi:
	CC -O0  -ansi -LANG:std -mips4 -n32 -D_POSIX_C_SOURCE=199506L -D_STL_PORT -I/home/igt2293/stapl/include -I/home/igt2293/stapl/include/stlport main.cc -L/home/igt2293/stapl/lib -lstlport_mipspro
sgi2:
	CC -O0  -ansi -LANG:std -mips4 -n32 -D_POSIX_C_SOURCE=199506L -D__SGI main.cc

sun:
	CC -o exe -D_gplusplus  -D_EXPLICIT_USE_NAMESPACE_STD main.cc
sungcc:
	g++ -o exe -g -D_gplusplus main.cc

clean:
	rm -rf *.o exe oldexe core
