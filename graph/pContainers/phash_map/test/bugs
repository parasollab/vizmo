  virtual void AddPart2LocationMap(const pContainer_Part_type& _bt) {
    for(part_const_iterator it = _bt.begin(); it!=_bt.end(); ++it) {
      PARTID _partid = _bt.GetPartId();
      Location location(myid,_partid);
      GID _gid;// = it->GetGid();               //TODOD TODO TODO
      pair<GID,Location> tp(_gid,location);
      dist.Add2LocationMap(tp);
    }  
  }



