	   GUI/FileListDialog.o \
	   GUI/FileOptions.o \
	   GUI/GLWidget.o \
	   GUI/GLWidgetOptions.o \
	   GUI/HelpOptions.o \
	   GUI/MainMenu.o \
	   GUI/MainWindow.o \
	   GUI/ModelSelectionWidget.o \
	   GUI/MovieSaveDialog.o \
	   GUI/NodeEditDialog.o \
	   GUI/PathOptions.o \
	   GUI/QueryGUI.o \
	   GUI/RoadmapOptions.o \
	   GUI/RobotOptions.o \
	   GUI/SliderDialog.o \
	   GUI/TextGUI.o

####### MOC
.SECONDARY: $(SRCMOC)

moc_%.cpp: ../%.h
	$(MOC) $< -o $@
